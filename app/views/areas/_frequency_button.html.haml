- levels = {"High" => 3, "Medium" => 2, "Low" => 1}
.btn-group.btn-group-xs
  != area_button(area_id,"Frequency")
  - if current_user.try(:admin?)
    %ul.dropdown-menu{:role => "menu"}
      - levels.each do |l,n|
        %li
          =link_to areas_set_area_frequency_path(:area_id => area_id, :frequency => n), :remote => true, :method => :post do
            = l
